{
    "name": "manajet/laravel-extra-resource",
    "description": "This packages allows you to add extra parameters to your Eloquent API resources and collections.",
    "keywords": [
        "laravel",
        "extra",
        "resource",
        "manajet"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dimitri Bocquet",
            "email": "contact@dimitribocquet.com",
            "homepage": "https://dimitribocquet.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manajet\\ExtraResource\\Providers\\LaravelServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Manajet\\ExtraResource\\": "src/"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
